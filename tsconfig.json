{
  "compileOnSave": false,
  "angularCompilerOptions": {
    "strictTemplates": true
  },
  "compilerOptions": {
    "downlevelIteration": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "alwaysStrict": true,
    "baseUrl": "./",
    "importHelpers": true,
    "module": "es2020",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2020",
      "dom"
    ],
    "paths": {
      "ui": [
        "dist/ui"
      ],
      "ui/*": [
        "dist/ui/*"
      ],
      "@dwn/*": [
        "src/app/shared/*"
      ]
    }
  }
}