{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
        "typedef": [
            true,
            "call-signature"
        ],
        "array-type": [
            true,
            "generic"
        ],
        "arrow-parens": true
    }
}